--build-arg REACT_APP_AWS_USER_POOLS_ID="us-east-1_0RSrdDus3" \
--build-arg REACT_APP_CLIENT_ID="3rli4op0d6u9ajb7rrtm5sdsud" \
-t frontend-react-js \
-f Dockerfile.prod \
.